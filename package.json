{
  "name": "templiteral",
  "author": "Caleb D. Williams <caleb.d.williams@gmail.com>",
  "keywords": [
    "template literal",
    "template string",
    "javascript",
    "html",
    "template",
    "DOM",
    "render",
    "reactive",
    "web component",
    "custom element",
    "customElements",
    "Angular",
    "React",
    "component",
    "template engine"
  ],
  "version": "3.6.2",
  "description": "",
  "main": "dist/templiteral.min.js",
  "module": "dist/templiteral.es.min.js",
  "scripts": {
    "prebuild": "npm test && npm run clean",
    "build": "rollup -c",
    "postbuild": "uglifyjs dist/templiteral.js -o dist/templiteral.min.js -m && uglifyjs dist/templiteral.cjs.js -o dist/templiteral.cjs.min.js -m && uglifyjs dist/templiteral.umd.js -o dist/templiteral.umd.min.js -m && uglifyjs dist/templiteral.es.js -o dist/templiteral.es.min.js -m",
    "clean": "rimraf dist",
    "prerelease": "npm run build",
    "release": "standard-version",
    "postrelease": "git push --follow-tags origin master; npm publish",
    "start": "node server.js",
    "pretest": "rm -Rf coverage",
    "test": "karma start"
  },
  "license": "SEE LICENSE IN LICENSE.md",
  "dependencies": {
    "stylit": "^0.3.0"
  },
  "devDependencies": {
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "cross-env": "^5.2.0",
    "eslint": "^4.19.1",
    "express": "^4.16.3",
    "express-middleware-rollup": "^1.1.1",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jasmine-core": "^2.99.1",
    "karma": "^1.7.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.2",
    "karma-coverage-istanbul-reporter": "^1.4.3",
    "karma-jasmine": "^1.1.2",
    "karma-rollup-plugin": "^0.2.4",
    "karma-rollup-preprocessor": "^5.1.1",
    "rollup": "^0.50.1",
    "standard-version": "^4.4.0",
    "uglify-es": "^3.3.10"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/calebdwilliams/templiteral"
  }
}

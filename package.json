{
  "name": "templiteral",
  "author": "Caleb D. Williams <caleb.d.williams@gmail.com>",
  "keywords": [
    "template literal",
    "template string",
    "javascript",
    "html",
    "template",
    "DOM",
    "render",
    "reactive",
    "web component",
    "custom element",
    "customElements",
    "Angular",
    "React"
  ],
  "version": "2.5.0",
  "description": "",
  "main": "dist/templiteral.js",
  "module": "dist/templiteral.es.js",
  "scripts": {
    "prebuild": "npm run clean",
    "build": "rollup -c",
    "postbuild": "uglifyjs dist/templiteral.js -o dist/templiteral.min.js -m && uglifyjs dist/templiteral.cjs.js -o dist/templiteral.cjs.min.js -m && uglifyjs dist/templiteral.umd.js -o dist/templiteral.umd.min.js -m && uglifyjs dist/templiteral.es.js -o dist/templiteral.es.min.js",
    "clean": "rimraf dist",
    "release": "standard-version",
    "postrelease": "git push --follow-tags origin master; npm publish",
    "start": "node server.js",
    "test": "karma start"
  },
  "license": "SEE LICENSE IN LICENSE.md",
  "dependencies": {},
  "devDependencies": {
    "body-parser": "^1.18.2",
    "cross-env": "^5.1.0",
    "eslint": "^4.10.0",
    "express": "^4.16.2",
    "express-middleware-rollup": "^1.1.1",
    "istanbul-instrumenter-loader": "^3.0.0",
    "jasmine-core": "^2.8.0",
    "karma": "^1.7.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-jasmine": "^1.1.0",
    "karma-rollup-preprocessor": "^5.0.1",
    "rollup": "^0.50.0",
    "rollup-plugin-uglify-es": "0.0.1",
    "standard-version": "^4.2.0",
    "uglify-es": "^3.1.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/calebdwilliams/templiteral"
  }
}

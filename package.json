{
  "name": "templit",
  "author": "Caleb D. Williams <caleb.d.williams@gmail.com>",
  "version": "1.1.1",
  "description": "",
  "main": "dist/templit.js",
  "module": "dist/templit.esm.js",
  "scripts": {
    "prebuild": "npm run clean",
    "build": "rollup -c",
    "postbuild": "uglifyjs dist/templit.js -o dist/templit.min.js -m && uglifyjs dist/templit.cjs.js -o dist/templit.cjs.min.js -m && uglifyjs dist/templit.umd.js -o dist/templit.umd.min.js -m && uglifyjs dist/templit.es.js -o dist/templit.es.min.js",
    "clean": "rimraf dist",
    "release": "standard-version",
    "start": "node server.js",
    "test": "karma start"
  },
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "body-parser": "^1.18.2",
    "cross-env": "^5.1.0",
    "express": "^4.16.2",
    "express-middleware-rollup": "^1.1.1",
    "istanbul-instrumenter-loader": "^3.0.0",
    "jasmine-core": "^2.8.0",
    "karma": "^1.7.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-jasmine": "^1.1.0",
    "karma-rollup-preprocessor": "^5.0.1",
    "rollup": "^0.50.0",
    "rollup-plugin-uglify-es": "0.0.1",
    "standard-version": "^4.2.0",
    "uglify-es": "^3.1.5"
  }
}
